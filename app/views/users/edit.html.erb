<h1>Editing user</h1>

<% form_for(@user) do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label "full name" %><br />
    <%= f.text_field :full_name %>
  </p>
  <p>
    <%= f.label :bio %><br />
    <%= f.text_area :bio %>
  </p>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>
